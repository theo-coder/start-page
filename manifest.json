{
    "manifest_version": 2,
    "name": "Start Page",
    "version": "1.0.0",
    "description": "A Firefox extension for a custom start page.",
    "developer": {
      "name": "theo-coder",
      "url": "https://github.com/theo-coder/start-page"
    },
    "permissions": [
      "storage"
    ],
    "optional_permissions": [
      "<all_urls>",
      "webRequest",
      "webRequestBlocking"
    ],
    "chrome_url_overrides" : {
      "newtab": "index.html"
    }
  }